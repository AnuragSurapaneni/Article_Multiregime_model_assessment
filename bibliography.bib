	pages = {1507--1529},
	file = {Scholtissek et al. - 2015 - A multi-scale asymptotic scaling and regime analys.pdf:C\:\\Users\\carlo\\Zotero\\storage\\L42WLSU2\\Scholtissek et al. - 2015 - A multi-scale asymptotic scaling and regime analys.pdf:application/pdf},
