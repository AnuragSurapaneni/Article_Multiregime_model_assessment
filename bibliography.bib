	author = {Laera, D. and Agostinelli, P.W. and Selle, L. and Cazères, Q. and Oztarlik, G. and Schuller, T. and Gicquel, L. and Poinsot, T.},
	year = {2021},
	pages = {6355--6363},
	file = {Laera et al. - 2021 - Stabilization mechanisms of CH4 premixed swirled f.pdf:C\:\\Users\\carlo\\Zotero\\storage\\QTJYL3F8\\Laera et al. - 2021 - Stabilization mechanisms of CH4 premixed swirled f.pdf:application/pdf},
